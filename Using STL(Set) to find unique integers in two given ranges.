#include <iostream>
#include<set>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	   set<int>val;
	   int a,b,c,d,k=0;
	   cin>>a>>b>>c>>d;
	   
	   for(int i=a;i<=b;i++)val.insert(i);
	   for(int i=c;i<=d;i++)val.insert(i);
	   
	   for(int i:val)k++;
	   cout<<k<<endl;
	}
	
	return 0;
}
